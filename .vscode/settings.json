{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-ally.localesPaths": ["i18n"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.enabledFrameworks": ["i18next", "next-intl"],
  "i18n-ally.namespace": true,
  "i18n-ally.pathMatcher": "{locale}/**/*.json",
  "i18n-ally.sortKeys": true,
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.extract.keygenStyle": "nested",
  "i18n-ally.keysInUse": ["*.title", "*.description", "*.button", "*.label", "*.placeholder", "*.message", "*.error"],
  "i18n-ally.missingTranslation.enabled": true,
  "i18n-ally.review.enabled": true,
  "i18n-ally.review.showInReviewMode": ["missing", "empty"],
  "i18n-ally.dirStructure": "auto",
  "i18n-ally.translate.engines": ["google", "deepl"],
  "git.ignoreLimitWarning": true,
  "terminal.integrated.rendererType": "dom"
}
